PUT     /admin/:thing/$what<.*>         scrupal.admin.AdminController.doPUT(thing,what)
POST    /admin/:thing/$what<.*>         scrupal.admin.AdminController.doPOST(thing,what)
HEAD    /admin/:thing/$what<.*>         scrupal.admin.AdminController.doHEAD(thing,what)
OPTIONS /admin/:thing/$what<.*>         scrupal.admin.AdminController.doOPTIONS(thing,what)
DELETE  /admin/:thing/$what<.*>         scrupal.admin.AdminController.doDELETE(thing,what)

GET     /assets/webjar/:wj/*file        scrupal.core.Assets.webjar(wj, file)
GET     /assets/stylesheet/*file        scrupal.core.Assets.css(file : Asset, path: String = "/public")
GET     /assets/javascript/*file        scrupal.core.Assets.js(file : Asset, path : String = "/public")
GET     /assets/projectjs/*file         scrupal.core.Assets.projectjs(file)
GET     /assets/image/*file             scrupal.core.Assets.img(file : String)
GET     /assets/theme/:theme            scrupal.core.Assets.theme(theme : String)
GET     /assets/theme/:provider/:theme  scrupal.core.Assets.themeFromProvider(provider : String, theme: String)
GET     /assets/*file                   scrupal.core.Assets.at(file : String)
