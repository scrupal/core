GET     /api/:entity/$what<.*>          scrupal.core.APIController.doGET(entity, what)
PUT     /api/:entity/$what<.*>          scrupal.core.APIController.doPUT(entity, what)
POST    /api/:entity/$what<.*>          scrupal.core.APIController.doPOST(entity, what)
HEAD    /api/:entity/$what<.*>          scrupal.core.APIController.doHEAD(entity, what)
OPTIONS /api/:entity/$what<.*>          scrupal.core.APIController.doOPTIONS(entity, what)
DELETE  /api/:entity/$what<.*>          scrupal.core.APIController.doDELETE(entity, what)

GET     /app/:app/$what<.*>             scrupal.core.ApplicationController.doGET(app,what)
PUT     /app/:app/$what<.*>             scrupal.core.ApplicationController.doPUT(app,what)
POST    /app/:app/$what<.*>             scrupal.core.ApplicationController.doPOST(app,what)
HEAD    /app/:app/$what<.*>             scrupal.core.ApplicationController.doHEAD(app,what)
OPTIONS /app/:app/$what<.*>             scrupal.core.ApplicationController.doOPTIONS(app,what)
DELETE  /app/:app/$what<.*>             scrupal.core.ApplicationController.doDELETE(app,what)

